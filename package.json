{
  "name": "compose-regexp",
  "version": "0.5.3",
  "description": "A set of functions to build and compose complex regular expressions",
  "type": "module",
  "main": "./commonjs/compose-regexp.js",
  "module": "./src/main.js",
  "jsnext:main": "./src/main.js",
  "exports": {
    ".": {
      "require": "./commonjs/compose-regexp.js",
      "default": "./src/main.js"
    }
  },
  "scripts": {
    "test": "gosub cjsify && gosub ospec",
    "ospec": "node --experimental-conditional-exports ./node_modules/.bin/ospec --module",
    "build": "gosub cjsify && gosub uglify && gosub gzip",
    "cjsify": "rollup src/export-all.js  -f umd -n composeRegexp -u compose-regexp | buble > commonjs/compose-regexp.js",
    "uglify": "uglifyjs commonjs/compose-regexp.js -cm --preamble '/**@license MIT-compose-regexp.js-©Pierre-Yves Gérardy*/' > dist/compose-regexp.min.js",
    "gzip": "cat dist/compose-regexp.min.js | gzip -9 > dist/compose-regexp.min.js.gz"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/pygy/compose-regexp.js.git"
  },
  "keywords": [
    "compose",
    "regexp",
    "builder",
    "regular",
    "expression"
  ],
  "author": "Pierre-Yves Gérardy",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/pygy/compose-regexp.js/issues"
  },
  "homepage": "https://github.com/pygy/compose-regexp.js#readme",
  "devDependencies": {
    "buble": "^0.19.8",
    "gosub": "1.0.0",
    "ospec": "pygy/ospec#module-and-parallel",
    "rollup": "^0.24.0",
    "uglify-js": "^2.6.1"
  }
}
